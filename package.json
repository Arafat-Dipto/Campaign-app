{
	"private": true,
	"scripts": {
		"dev": "npm run development",
		"development": "mix",
		"watch": "mix watch",
		"watch-poll": "mix watch -- --watch-options-poll=1000",
		"hot": "mix watch --hot",
		"prod": "npm run production",
		"production": "mix --production",
		"watch-frontend": "mix watch --mix-config=webpack.frontend.mix.js",
		"hot-frontend": "mix watch --hot --mix-config=webpack.frontend.mix.js",
		"production-frontend": "mix --mix-config=webpack.frontend.mix.js --production",
		"watch-backend": "mix watch --mix-config=webpack.backend.mix.js",
		"hot-backend": "mix watch --hot --mix-config=webpack.backend.mix.js",
		"production-backend": "mix --mix-config=webpack.backend.mix.js --production",
		"production-all": "npm run production-frontend && npm run production-backend"
	},
	"dependencies": {
